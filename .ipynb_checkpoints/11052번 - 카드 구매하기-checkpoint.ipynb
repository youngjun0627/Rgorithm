   "source": [
    "n = int(input())\n",
    "\n",
    "cards = list(map(int,input().split()))\n",
    "cards.insert(0,0)\n",
    "\n",
    "dp = [0 for _ in range(n+1)]\n",
    "for i in range(n+1):\n",
    "    dp[i]=cards[i]\n",
    "    for j in range(i):\n",
    "        dp[i] = max(dp[i], dp[i-j]+cards[j])\n",
    "print(dp[-1])"
   ]
