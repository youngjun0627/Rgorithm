    "dp = [0 for _ in range(n+1)]\n",
    "dp[0]=1\n",
    "dp[1]=3\n",
    "\n",
    "\n",
    "if n==1:\n",
    "    print(dp[1])\n",
    "\n",
    "else:\n",
    "    for i in range(2,n+1):\n",
    "        dp[i] = (dp[i-1] * 2 + dp[i-2])%9901\n",
    "        \n",
    "print(dp[n]%9901)\n"
